function person$grow_up
	load_callback age
	int 1
	bin_op +
	store_object age

	ret
end

function person
	arg 0
	store name
	arg 1
	store age

	make_object name age

	ret
end

function main
	string "mateo"
	int 16

	call src/bytecode/bin/test.mmm#person
	
	store me

	load_local me

	call src/bytecode/bin/test.mmm#get_older

	load_local me

	string "mark"
	mutate name

	printn *

	ret
end

function get_older
	arg 0
	printn *
	call_object src/bytecode/bin/test.mmm#person$grow_up
	printn *

	void
	ret
end